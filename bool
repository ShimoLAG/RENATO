BExp ::=  expr '<=' BExp
            | expr '==' BExp
            | expr '>=' BExp
            | expr '<' BExp
            | expr '>' BExp
            | expr

    expr ::= term '+' expr 
       | term '-' expr
       | term
       
    term ::= factor '*' term
       | factor
       
    factor ::= '(' expr ')'
        | number

    number ::= nat

    nat ::= digit {digit} # one or more digit (many1)

    nat ::= digit+
